#include <ylessinc\YSI_Coding\y_hooks>

bool:IsVehicleBicycle(vehicle) {

	switch(vehicle) {

		case 481, 509, 510: return true;
	}
	return false;
}

//* Prikolice
stock bool:IsVehicleTrailer(vehicle) {

	switch(vehicle) {

		case 435, 450, 584, 591, 606, 607, 610, 611: return true;
	}
	return false;
}